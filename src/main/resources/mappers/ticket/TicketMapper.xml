<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dev.jwkim.jgv.mappers.Ticket.TicketMapper">

    <insert id="insertSeat"  parameterType="dev.jwkim.jgv.entities.Ticket.SeatEntity">
        insert into `jgv`.`seat`
            (`se_num`, `se_name`, `ci_num`)
        values (#{seNum}, #{seName}, #{ciNum})
    </insert>
</mapper>