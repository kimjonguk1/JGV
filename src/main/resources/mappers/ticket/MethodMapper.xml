<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dev.jwkim.jgv.mappers.ticket.MethodMapper">

    <select id="selectPaymentMeNum" resultType="dev.jwkim.jgv.entities.ticket.MethodEntity">
        select me_num as `meNum`,
               me_name as `meName`
        from `jgv`.`method`
        where `me_name` = #{meName}
        limit 1
    </select>
</mapper>