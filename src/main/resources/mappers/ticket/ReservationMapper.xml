<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dev.jwkim.jgv.mappers.Ticket.ReservationMapper">
    <insert id="insertReservation" parameterType="dev.jwkim.jgv.entities.ticket.ReservationEntity">
        insert into `jgv`.reservation(`re_num`, `sc_num`, `se_num`, `pa_num`)
            values (${reNum}, ${scNum}, ${seNum}, ${paNum})
    </insert>
</mapper>