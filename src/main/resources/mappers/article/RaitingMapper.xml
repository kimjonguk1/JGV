<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dev.jwkim.jgv.mappers.movie.RaitingMapper">

    <insert id="insertMovieRaiting">
        INSERT INTO jgv.rating (mo_num, ra_grade)
        VALUES (#{moNum}, #{ra_grade})
    </insert>
    <delete id="deleteAllMovieRaiting">
        DELETE FROM jgv.rating
    </delete>
</mapper>