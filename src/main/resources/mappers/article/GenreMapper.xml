        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dev.jwkim.jgv.mappers.movie.GenreMapper">

    <insert id="insertMovieGenre">
        INSERT INTO jgv.genre (ge_name)
        VALUES (#{geName})
